    "body-parser": "^1.19.0",
    "css-loader": "^5.0.2",
    "enzyme": "^3.11.0",
    "expect": "^26.6.2",
    "express": "^4.17.1",
    "fibers": "^5.0.0",
    "handlebars": "^4.7.7",
    "mocha": "^8.3.0",
    "node": "^15.8.0",
    "node-sass": "^5.0.0",
    "nodemailer-smtp-transport": "^2.7.4",
